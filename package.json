    "build": "cross-env NODE_ENV=production webpack"
  },
  "dependencies": {
    "link": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.9.7",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
