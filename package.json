{
  "name": "frontend-guide",
  "description": "Webpack 5 boilerplate - Main branch of the frontend-guide repository on GitHub. The page topic is a learning syllabus and roadmap with resource links.",
  "keywords": [
    "Webpack"
  ],
  "author": "Coding Sable",
  "license": "ISC",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "frontend-guide"
  },
  "main": "index.js",
  "engines": {
    "npm": ">=10.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run jest"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "echo": "echo \"Error: no test specified\" && exit 1",
    "bundle": "webpack",
    "watch": "webpack --watch",
    "start": "webpack serve",
